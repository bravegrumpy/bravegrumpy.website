{
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[svelte]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
    },
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": false
    },
    "editor.formatOnSave": true,
    "files.associations": {
      "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
      "other": "on",
      "comments": "off",
      "strings": "on"
    },
    "tailwindCSS.includeLanguages": {
      "plaintext": "html"
    },
    "tailwindCSS.colorDecorators": true,
    "editor.colorDecorators": true,
    "editor.fontSize": 16,
    "editor.fontFamily": "'FantasqueSansM Nerd Font','Cedarville_Cursive', 'Daddytime Mono', Consolas, 'Courier New', monospace"
}
  