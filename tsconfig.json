{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths" : {
        "@/*": ["./src/*"],
        "@components/*" : ["./src/components/*"],
        "@layouts/*": ["./src/layouts/*"],
        "@styles/*" : ["./src/styles/*"],
        "@scripts/*" : ["./src/scripts/*"],
    },
    "strictNullChecks": true,
    "allowJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
  }
}